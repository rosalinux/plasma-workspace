#: package/contents/ui/DeviceItem.qml:180
#, kde-format
msgctxt "@info:status Free disk space"
msgid "%1 free of %2"
msgstr "%2-இல் %1 காலி"

#: package/contents/ui/DeviceItem.qml:184
#, kde-format
msgctxt ""
"Accessing is a less technical word for Mounting; translation should be short "
"and mean 'Currently mounting this device'"
msgid "Accessing…"
msgstr "அணுகப்படுகிறது…"

#: package/contents/ui/DeviceItem.qml:187
#, kde-format
msgctxt ""
"Removing is a less technical word for Unmounting; translation should be "
"short and mean 'Currently unmounting this device'"
msgid "Removing…"
msgstr "நீக்கப்படுகிறது…"

#: package/contents/ui/DeviceItem.qml:190
#, kde-format
msgid "Don't unplug yet! Files are still being transferred..."
msgstr "கழற்றாதீர்கள்! கோப்புகள் இன்னும் இடமாற்றப்படுகின்றன…"

#: package/contents/ui/DeviceItem.qml:221
#, kde-format
msgid "Open in File Manager"
msgstr "கோப்பு மேலாளரில் திற"

#: package/contents/ui/DeviceItem.qml:224
#, kde-format
msgid "Mount and Open"
msgstr "ஏற்றி திற"

#: package/contents/ui/DeviceItem.qml:226
#, kde-format
msgid "Eject"
msgstr "வெளியேற்று"

#: package/contents/ui/DeviceItem.qml:228
#, kde-format
msgid "Safely remove"
msgstr "பத்திரமாக நீக்கு"

#: package/contents/ui/DeviceItem.qml:270
#, kde-format
msgid "Mount"
msgstr "ஏற்று"

#: package/contents/ui/devicenotifier.qml:47
#, kde-format
msgid "Most Recent Device"
msgstr "மிக சமீபமான சாதனம்"

#: package/contents/ui/devicenotifier.qml:47
#, kde-format
msgid "No Devices Available"
msgstr "எந்த சாதனமும் இல்லை"

#: package/contents/ui/devicenotifier.qml:238
#: package/contents/ui/FullRepresentation.qml:37
#, kde-format
msgid "Remove All"
msgstr "அனைத்தையும் நீக்கு"

#: package/contents/ui/devicenotifier.qml:245
#, kde-format
msgid "Removable Devices"
msgstr "கழற்றக்கூடிய சாதனங்கள்"

#: package/contents/ui/devicenotifier.qml:251
#, kde-format
msgid "Non Removable Devices"
msgstr "கழற்ற முடியாத சாதனங்கள்"

#: package/contents/ui/devicenotifier.qml:257
#, kde-format
msgid "All Devices"
msgstr "அனைத்து சாதனங்கள்"

#: package/contents/ui/devicenotifier.qml:265
#, kde-format
msgid "Show popup when new device is plugged in"
msgstr "புதிய சாதனம் செருகப்படும்போது தெரித்தெழும் பட்டியை காட்டு"

#: package/contents/ui/devicenotifier.qml:274
#, kde-format
msgctxt "Open auto mounter kcm"
msgid "Configure Removable Devices…"
msgstr "கழற்றக்கூடிய சாதனங்களை அமை…"

#: package/contents/ui/FullRepresentation.qml:40
#, kde-format
msgid "Click to safely remove all devices"
msgstr "அனைத்து சாதனங்களையும் பத்திரமாக நீக்க க்ளிக் செய்யவும்"

#: package/contents/ui/FullRepresentation.qml:176
#, kde-format
msgid "No removable devices attached"
msgstr "கழற்றக்கூடிய சாதனங்கள் ஏதுமில்லை"

#: package/contents/ui/FullRepresentation.qml:176
#, kde-format
msgid "No disks available"
msgstr "வட்டுகள் ஏதுமில்லை"