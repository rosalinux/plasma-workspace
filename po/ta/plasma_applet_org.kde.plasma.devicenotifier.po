#: package/contents/ui/DeviceItem.qml:206
#, kde-format
msgid "Mount and Open"
msgstr "ஏற்றி திற"

#: package/contents/ui/DeviceItem.qml:208
#, kde-format
msgid "Eject"
msgstr "வெளியேற்று"

#: package/contents/ui/DeviceItem.qml:210
#, kde-format
msgid "Safely remove"
msgstr "பத்திரமாக நீக்கு"

#: package/contents/ui/DeviceItem.qml:250
#, kde-format
msgid "Mount"
msgstr "ஏற்று"

#: package/contents/ui/devicenotifier.qml:47
#, kde-format
