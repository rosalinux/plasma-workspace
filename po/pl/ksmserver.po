#: main.cpp:117
#, kde-format
msgid "$HOME not set!"
msgstr "$HOME nieustawiony!"

#: main.cpp:121 main.cpp:127
#, kde-format
msgid "$HOME directory (%1) does not exist."
msgstr "Katalog $HOME (%1) nie istnieje."

#: main.cpp:123
#, kde-format
