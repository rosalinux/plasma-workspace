#: logout.cpp:350
#, kde-format
msgid "Logout canceled by '%1'"
msgstr "“%1”取消了注销"

#: main.cpp:37
#, kde-format
msgid ""
"The reliable Plasma session manager that talks the standard X11R6 \n"
"session management protocol (XSMP)."
msgstr ""
"遵循标准 X11R6 会话管理协议(XSMP)的\n"
"可靠的Plasma会话管理器。"

#: main.cpp:113
#, kde-format
msgid "$HOME not set!"
msgstr "$HOME 未设置！"

